import java.util.*;
clas Series{
public static void main(String[] args){
Scannenr s =new Scanner(system.in);
System.out.println("Enter the number of terms of the fibonacci series");
int num = s.nextInt();
int sum=0;
int a=0;
int b=1;
for(int i=1;i<=num;i++){
System.out.println(a + ",");
sum=a+b;
a=b;
b=sum;
}
}
}
